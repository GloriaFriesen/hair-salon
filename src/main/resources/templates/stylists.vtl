<!--stylists.vtl-->

#if ($stylists.size() >= 1)
  <h1>Current Stylists</h1>
  #foreach ($stylist in $stylists)
    <h3><a href="/stylists/$stylist.getId()">$stylist.getName()</a></h3>
  #end
#else
  <h1>No stylists.</h1>
#end

<a href="/stylists/new"><button type="button" class="btn btn-success">Add Stylist</button></a>
